            android:textColor="@color/white"
            android:textSize="30sp" >
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"        
