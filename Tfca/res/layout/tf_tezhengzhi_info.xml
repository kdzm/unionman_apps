            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="290dp"
            android:textColor="@color/white"
            android:textSize="24sp" >

        </TextView>

        <TextView
            android:id="@+id/textView11"
