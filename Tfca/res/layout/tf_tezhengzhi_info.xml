            android:textColor="@color/white"
            android:textSize="24sp" >
        </TextView>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="right"
            android:textColor="@color/white"
            android:textSize="24sp" >
        </TextView>
