<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.um.dvbsettings"
    android:versionCode="1"
    android:versionName="1.0"
    android:sharedUserId="android.uid.system">
	
    <uses-sdk
        android:minSdkVersion="16" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
	<uses-permission android:name="android.permission.WRITE_SETTINGS"/>

    
    <application
        android:icon="@drawable/icon"
         android:theme="@style/AppThemeNew" 
        android:label="@string/app_name_private" >

         <activity
            android:name="com.um.ui.AndoroidApp"
            android:label="@string/app_name" >
        </activity>

              
        <activity
            android:name="com.um.ui.SysSetting"
            android:launchMode="singleInstance"
           
            android:label="@string/app_name_private" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.um.ui.Freset"
            android:label="@string/app_name" >
        </activity>


        <activity
            android:name="com.um.ui.SignCheck"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.um.ui.ParentLock"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.um.ui.AudioSetting"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.um.ui.BootDVBActivity"
            android:label="@string/app_name" >
        </activity>
		
        <activity
            android:name="com.um.ui.TunerSetting"
            android:label="@string/app_name" >
        </activity>
  
        <activity
            android:name="com.um.ui.ProgramInfo"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.um.ui.ChannelStopMode"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.um.ui.SoundChannelSetting"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
