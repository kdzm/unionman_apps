<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
    <TextView android:focusable="false" android:layout_height="54dip" android:id="@+id/programTip" android:gravity="center" android:textSize="25sp" android:layout_width="80dip"></TextView>
	<TextView
		android:focusable="false"
		android:id="@+id/programName"
		android:layout_height="54dip"
		android:textColor="#ffffff"
		android:textSize="25sp"
		android:gravity="center|left" android:layout_width="280dip"/>

</LinearLayout>
