<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WifiEncryptionState">
        <attr name="state_encrypted" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CoverFlow">
        <attr name="animateDuration" format="integer" />
        <attr name="delayTime" format="integer" />
        <attr name="images" format="integer" />
    </declare-styleable>
    <declare-styleable name="IpAddrEdit">
        <attr
            name="text"
            format="string" />
    </declare-styleable>

    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
</resources>
