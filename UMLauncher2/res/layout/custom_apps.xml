<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.um.launcher"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app01"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        android:layout_toRightOf="@+id/app05"
        app:av_title_bg="@drawable/app_06_title_bg"
        app:av_icon_bg="@drawable/app_bg_06"
        app:av_icon="@drawable/page_allapp"
        app:av_title="@string/more_app"
        android:layout_alignTop="@+id/first_item_window"
        app:av_icon_type="icon"
        android:layout_marginLeft="@dimen/launcher_item_margin_left"
        />
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app02"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        app:av_title_bg="@drawable/app_01_title_bg"
        app:av_icon_bg="@drawable/app_bg_01"
        app:av_title="@string/filemanage_normal"
        app:av_icon="@drawable/page_filemanage"
        app:av_icon_type="icon"
        android:visibility="invisible"
        android:layout_below="@id/custome_app01"
        android:layout_alignLeft="@id/custome_app01"
        android:layout_marginTop="@dimen/launcher_item_margin_top"
        />
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app03"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        app:av_title_bg="@drawable/app_02_title_bg"
        app:av_icon_bg="@drawable/app_bg_02"
        app:av_title="@string/multi_normal"
        app:av_icon="@drawable/page_multiscreen"
        app:av_icon_type="icon"
        android:visibility="invisible"
        android:layout_alignTop="@id/custome_app01"
        android:layout_toRightOf="@id/custome_app01"
        android:layout_marginLeft="@dimen/launcher_item_margin_left"
        />
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app04"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        app:av_title_bg="@drawable/app_03_title_bg"
        app:av_icon_bg="@drawable/app_bg_03"
        app:av_title="@string/media_normal"
        app:av_icon="@drawable/page_media"
        app:av_icon_type="icon"
        android:visibility="invisible"
        android:layout_alignTop="@id/custome_app02"
        android:layout_marginLeft="@dimen/launcher_item_margin_left"
        android:layout_toRightOf="@id/custome_app01"
        />
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app05"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        app:av_title_bg="@drawable/app_04_title_bg"
        app:av_icon_bg="@drawable/app_bg_04"
        app:av_title="@string/email_normal"
        app:av_icon="@drawable/page_browser"
        android:visibility="invisible"
        android:layout_alignTop="@id/custome_app01"
        android:layout_toRightOf="@id/custome_app04"
        android:layout_marginLeft="@dimen/launcher_item_margin_left"
        />
    <com.um.launcher.widget.AppView
        android:id="@+id/custome_app06"
        android:layout_width="@dimen/app_item_width_small"
        android:layout_height="@dimen/app_item_height_small"
        android:focusable="true"
        app:av_title_bg="@drawable/app_06_title_bg"
        app:av_icon_bg="@drawable/app_bg_06"
        app:av_icon="@drawable/jiaoyu"
        app:av_title="@string/tag_education"
        app:av_icon_type="icon"
        android:visibility="invisible"
        android:layout_alignTop="@id/custome_app02"
        android:layout_alignLeft="@id/custome_app05"
        />

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_alignLeft="@+id/custome_app01"
        android:layout_below="@id/custome_app02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        <ImageView
            android:id="@+id/custome_app_imgv_reflect01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <ImageView
            android:id="@+id/custome_app_imgv_reflect02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/launcher_item_margin_left"/>
        <ImageView
            android:id="@+id/custome_app_imgv_reflect03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/launcher_item_margin_left"/>
    </LinearLayout>
</merge>